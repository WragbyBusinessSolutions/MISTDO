@model MISTDO.Web.Models.ApplicationUser
@{
    ViewData["Title"] = "Profile";
    Layout = "_LayoutDashboard";
}

<link rel="stylesheet" href="~/bower_components/datatables/media/css/jquery.dataTables.css" />

<!-- Page Container START -->

<div class="page-container">
    <!-- Header START -->
    @await Html.PartialAsync("_LayoutHeader");

    <!-- Theme Toggle Button END -->
    <!-- Content Wrapper START -->
    <div class="main-content">
        <div class="container-fluid">
            <div class="page-title">
                <p>
                    <a asp-action="AllTrainingCenter" asp-route-id="@Model.Id">Back</a>
                </p>
                <h4>Account Setting</h4>

            </div>
          
            <div class="row">
                <div class="col-md-8">
                    <div class="row">
                      
                        <div class="col-md-10">
                            <div class="card">
                                <div class="card-heading border bottom">
                                    <div>  <h4 class="card-title">General Info</h4></div>
                                     <div align="right"> <a asp-action="EditProfile">Edit Your Profile</a></div>

                                </div>
                                <div class="col-md-push-10">


                                    <div class="card-block">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.FirstName)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <span class="status online mrg-top-10"></span>
                                                <p class="mrg-top-10"> @Html.DisplayFor(model => model.FirstName)</p>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.Email)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <span class="status online mrg-top-10"></span>
                                                <p class="mrg-top-10">@Html.DisplayFor(model => model.Email)</p>
                                            </div>
                                        </div>


                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b> @Html.DisplayNameFor(model => model.PhoneNumber)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <span class="status online mrg-top-10"></span>
                                                <p class="mrg-top-10">@Html.DisplayFor(model => model.PhoneNumber)</p>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.CompanyName)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.CompanyName)</span>
                                                </p>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.CompanyAddress)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.CompanyAddress)</span>
                                                </p>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.UserAddress)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.UserAddress)</span>
                                                </p>
                                            </div>
                                        </div>

                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.State)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.State)</span>
                                                </p>
                                            </div>
                                        </div>

                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.City)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.City)</span>
                                                </p>
                                            </div>
                                        </div>

                                        <hr>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <p class="mrg-top-10 text-dark"> <b>@Html.DisplayNameFor(model => model.CentreName)</b></p>
                                            </div>
                                            <div class="col-md-6">
                                                <p class="mrg-top-10">
                                                    <span class="status online mrg-top-10"></span>
                                                    <span class="mrg-left-10">@Html.DisplayFor(model => model.CentreName)</span>
                                                </p>
                                            </div>
                                        </div>

                                    </div>
                                </div>



                            </div>
                        </div>
</div>
            </div>
        </div>
    </div>
    <!-- Content Wrapper END -->
   
    <!-- Footer START -->
    <footer class="content-footer">
        <div class="footer">
            <div class="copyright">
                <span>Copyright © 2018 <img src="~/dist/assets/images/logo/footerlogo.png" width="12%" height="7%"></span>
                <span class="go-right">
                    <a href="#" class="text-gray mrg-right-15">Term &amp; Conditions</a>
                    <a href="#" class="text-gray">Privacy &amp; Policy</a>
                </span>
            </div>
        </div>
    </footer>
    <!-- Footer END -->
    <!-- Page Container END -->
</div>
<script src="~/dist/assets/js/vendor.js"></script>

<script src="~/dist/assets/js/app.min.js"></script>

<!-- page plugins js -->
<script src="~/bower_components/datatables/media/js/jquery.dataTables.js"></script>

<!-- page js -->
<script src="~/dist/assets/js/table/data-table.js"></script>



